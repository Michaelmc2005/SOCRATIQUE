#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/michaelmcmahon/Downloads/chatty-main/node_modules/.pnpm/svelte-check@3.0.4_gqx7lw3sljhsd4bstor5m2aa2u/node_modules/svelte-check/bin/node_modules:/Users/michaelmcmahon/Downloads/chatty-main/node_modules/.pnpm/svelte-check@3.0.4_gqx7lw3sljhsd4bstor5m2aa2u/node_modules/svelte-check/node_modules:/Users/michaelmcmahon/Downloads/chatty-main/node_modules/.pnpm/svelte-check@3.0.4_gqx7lw3sljhsd4bstor5m2aa2u/node_modules:/Users/michaelmcmahon/Downloads/chatty-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/michaelmcmahon/Downloads/chatty-main/node_modules/.pnpm/svelte-check@3.0.4_gqx7lw3sljhsd4bstor5m2aa2u/node_modules/svelte-check/bin/node_modules:/Users/michaelmcmahon/Downloads/chatty-main/node_modules/.pnpm/svelte-check@3.0.4_gqx7lw3sljhsd4bstor5m2aa2u/node_modules/svelte-check/node_modules:/Users/michaelmcmahon/Downloads/chatty-main/node_modules/.pnpm/svelte-check@3.0.4_gqx7lw3sljhsd4bstor5m2aa2u/node_modules:/Users/michaelmcmahon/Downloads/chatty-main/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
